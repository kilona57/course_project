section_of_psychiatry
	=> nrel_main_idtf:
		[Раздел. Предметная область психиатрии]
			(* <- lang_ru;; *);
		[Section. Subject domain of psychiatry] (* <- lang_en;; *);;
section_of_psychiatry<=nrel_section_decomposition:{
	        section_of_child_psychiatry;
		section_of_geriatric_psychiatry;//позднего возраста
		section_of_military_psychiatry;//военная
