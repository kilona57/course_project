};;		

section_of_psychiatry->rrel_key_sc_element:
        subject_domain_of_psychiatry;
        psychiatry;
        mentality;//психика
        psychiatrist;
        personality;//личность
        psychiatric_disorder;//расстройство
        psychotropic_substances;//психотропное средство 
        behaviour;//поведение
        emotions;//эмоции
        memory;//память
        thinking;//мышление
        conscience;//сознание
        hallucination;//галлюцинации
        delusion;//бред
        excitement;//возбуждение
        anxiety;
        disorders_mood;//расстройства настроения
        disorders_personality;//расстройство личности
        disorder_neurotic_disorders;//невротические расстройства
        behavioral_syndrome;//поведенческий синдром //шизоактивные расстройства
        schizoactive_disorders;;
section_of_psychiatry<-non_atomic_section;;